{
  "name": "humanchat4",
  "version": "1.0.0",
  "description": "HumanChat prototype workspace",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run web:build",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand --selectProjects client",
    "test:integration": "cross-env NODE_ENV=test jest --runInBand --selectProjects client --runTestsByPath apps/web/components/__tests__ apps/web/services/__tests__",
    "test:api": "cross-env NODE_ENV=test jest --runInBand --selectProjects server",
    "test:e2e": "playwright test",
    "test:ci": "npm run test && npm run test:api && npm run test:e2e",
    "clean": "rm -rf dist",
    "dev": "tsx src/server/index.ts",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "start": "node dist/src/server/index.js",
    "web:dev": "next dev apps/web --hostname 127.0.0.1 --port 3000",
    "web:build": "tsx scripts/web-build.ts",
    "web:start": "next start apps/web"
  },
  "keywords": [
    "humanchat",
    "dexie",
    "indexeddb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-virtual": "^3.13.12",
    "bcryptjs": "^3.0.3",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fake-indexeddb": "^6.2.5",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "next": "^16.0.3",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "recharts": "^2.12.7",
    "stripe": "^19.3.1",
